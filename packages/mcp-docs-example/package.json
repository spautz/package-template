{
  "name": "@spautz/mcp-docs-example",
  "version": "0.0.1",
  "description": "MCP server for accessing docs for the example libraries",
  "keywords": [],
  "license": "MIT",
  "homepage": "https://github.com/spautz/package-template/blob/main/packages/mcp-docs-example#readme",
  "bugs": "https://github.com/spautz/package-template/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spautz/package-template.git",
    "directory": "packages/mcp-docs-example"
  },
  "author": {
    "name": "Steven Pautz",
    "url": "https://stevenpautz.com"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "tag": "next"
  },
  "files": [
    "dist/",
    "../basic-library-template/docs/",
    "src/",
    "LICENSE",
    "*.md",
    "*.txt"
  ],
  "source": "./src/mcp-server.ts",
  "type": "module",
  "bin": "./dist/cli.js",
  "scripts": {
    "____ HOOKS _________________________________________________________": "",
    "prepack": "pnpm run clean && turbo run build",
    "postpack": "pnpm run build:verifypack",
    "____ STANDARD PACKAGE TASKS _________________________f_______________": "",
    "all": "turbo run pipeline-all",
    "all:ci": "turbo run pipeline-ci --ui=stream",
    "build": "pnpm run build:clean && pnpm run build:mcp && pnpm run build:verify",
    "clean": "pnpm run \"/^.*:clean$/\" && rimraf --glob ./node_modules/.cache ./.turbo ./*.log",
    "dev": "pnpm run build:lib:watch",
    "lint": "biome check --write .",
    "lint:verify": "biome check .",
    "publish:yalc": "yalc publish --sig --push --replace",
    "test:clean": "rimraf ./coverage",
    "test_DISABLED": "pnpm run test:clean       && vitest run --coverage",
    "test:ci_DISABLED": "pnpm run test:clean    && vitest run --coverage --bail 1",
    "test:ui_DISABLED": "pnpm run test:clean    && vitest --ui",
    "test:watch_DISABLED": "pnpm run test:clean && vitest watch --coverage",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "____ BUILD & VALIDATION TASKS ______________________________________": "",
    "build:clean": "rimraf --glob ./dist ./*.tgz",
    "build:mcp": "tsdown --config ./tsdown.config.ts",
    "build:lib:watch": "tsdown --config ./tsdown.config.ts --watch",
    "build:verify": "publint --strict",
    "build:verifypack": "ls *.tgz | xargs -I {} attw {}"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "catalog:",
    "publint": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@spautz/basic-library-template": "workspace:^",
    "@spautz/react-library-template": "workspace:^"
  },
  "peerDependenciesMeta": {
    "@spautz/basic-library-template": {
      "optional": true
    },
    "@spautz/react-library-template": {
      "optional": true
    }
  }
}
