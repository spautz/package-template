{
  "name": "@package-template/demo-template-react18",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "____ INTEGRATION ___________________________________________________": "",
    "all": "pnpm run clean && pnpm run types && pnpm run test:coverage && pnpm run build",
    "all:readonly": "pnpm run types && pnpm run test:coverage",
    "clean": "pnpm run build:clean && pnpm run test:clean && rimraf ./node_modules/.cache/ *.log",
    "____ MAIN WORKFLOW _________________________________________________": "",
    "start": "vite dev",
    "build": "pnpm run build:clean && pnpm run build:main",
    "test": "pnpm run test:coverage",
    "____ INDIVIDUAL COMMANDS ___________________________________________": "",
    "build:clean": "rimraf ./build",
    "build:main": "vite build",
    "test:clean": "rimraf ./coverage",
    "test:coverage": "pnpm run test:clean && rescripts test --coverage --watch=false --watchAll=false",
    "test:nowatch": "pnpm run test:clean && rescripts test --coverage=false --watch=false --watchAll=false --bail",
    "test:watch": "pnpm run test:clean && rescripts test --coverage=false --watch",
    "test:watchcoverage": "pnpm run test:clean && rescripts test --coverage --watchAll",
    "types": "tsc --noEmit --p tsconfig.json"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "vite": "^3.0.5",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
