{
  "name": "demo-name-goes-here",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "____ INTEGRATION ___________________________________________________": "",
    "all": "yarn run clean && yarn run types && yarn run test:coverage && yarn run build",
    "all:readonly": "yarn run types && yarn run test:coverage",
    "clean": "yarn run build:clean && yarn run test:clean && rimraf ./node_modules/.cache/ *.log",
    "____ MAIN WORKFLOW _________________________________________________": "",
    "start": "rescripts start",
    "build": "yarn run build:clean && yarn run build:main",
    "test": "yarn run test:coverage",
    "____ INDIVIDUAL COMMANDS ___________________________________________": "",
    "build:clean": "rimraf ./build",
    "build:main": "rescripts build",
    "test:clean": "rimraf ./coverage",
    "test:coverage": "yarn run test:clean && rescripts test --coverage --watch=false --watchAll=false",
    "test:nowatch": "yarn run test:clean && rescripts test --coverage=false --watch=false --watchAll=false --bail",
    "test:watch": "yarn run test:clean && rescripts test --coverage=false --watch",
    "test:watchcoverage": "yarn run test:clean && rescripts test --coverage --watchAll",
    "types": "tsc --noEmit --p tsconfig.json"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.16",
    "@rescripts/rescript-env": "^0.0.12",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "react-scripts": "5.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
